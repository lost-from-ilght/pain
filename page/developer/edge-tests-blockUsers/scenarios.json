    {
      "scenarioId": "2-A",
      "group": "User Blocks - Actions",
      "title": "Block  User to User with Scope",
      "description": "POST /user-blocks/blockUser - Lets one user permanently/temporarily block another user in a chosen area (like Feed or Private Messages).",
      "apiMethod": "POST",
      "apiEndpoint": "/user-blocks/blockUser",
      "requestPayload": {
        "from": "user_a",
        "to": "user_b",
        "scope": "feed",
        "is_permanent": true,
        "expires_at": null,
        "testing": true
      },
      "checklistItems": [
        "The block should only apply in the selected area (scope)"
      ],
      "inputFields": [
        {
          "id": "from",
          "label": "Who is doing the blocking (From)",
          "type": "text",
          "required": true,
          "value": "user_a"
        },
        {
          "id": "to",
          "label": "Who is being blocked (To)",
          "type": "text",
          "required": true,
          "value": "user_b"
        },
        {
          "id": "scope",
          "label": "Where should the block apply?",
          "type": "select",
          "required": true,
          "options": [
            { "value": "", "text": "global (all)" },
            { "value": "feed", "text": "Feed (posts, comments, timeline)" },
            { "value": "private_chat", "text": "Private Messages (DMs)" }
          ],
          "value": "feed"
        },
        {
          "id": "is_permanent",
          "label": "Is this a permanent block?",
          "type": "select",
          "options": [
            { "value": "false", "text": "No – Temporary" },
            { "value": "true", "text": "Yes – Permanent" }
          ],
          "value":false
        },
        {
          "id": "expires_at",
          "label": "Duration of the block (in seconds)",
          "type": "text",
          "required": false,
          "value": "86400",
          "placeholder": "e.g. 86400 = 24 hours",
          "inputMode": "numeric"
        }
      ]
    },

