{
  "scenarios": [
    {
      "scenarioId": "1-A",
      "group": "User Blocks - Listing",
      "title": "View All User Blocks Globally (with filters)",
      "description": "GET /user-blocks/listUserBlocks - Shows a list of all blocks between users. You can filter by scope (where the block applies), type of block, whether it's permanent, and use pagination to see more results.",
      "apiMethod": "GET",
      "apiEndpoint": "/user-blocks/listUserBlocks",
      "checklistItems": [
        "Test pagination works correctly (using limit + nextToken)",
        "Verify filtering works properly for from/to user, scope, flag, and permanent status",
        "The number of blocks shown should match the reported count"
      ],
      "inputFields": [
        {
          "id": "scope",
          "label": "Where the block applies (Scope)",
          "type": "select",
          "options": [
            { "value": "", "text": "Any / Global" },
            { "value": "feed", "text": "Feed (posts & comments)" },
            { "value": "private_chat", "text": "Private Messages (DMs)" }
          ]
        },
        {
          "id": "flag",
          "label": "Block Type / Reason (flag)",
          "type": "text",
          "placeholder": "e.g. manual, spam, harassment"
        },
        {
          "id": "is_permanent",
          "label": "Is this a permanent block?",
          "type": "select",
          "options": [
            { "value": "", "text": "Any (permanent or temporary)" },
            { "value": "true", "text": "Yes – Permanent" },
            { "value": "false", "text": "No – Temporary" }
          ]
        },
        {
          "id": "limit",
          "label": "Number of results per page",
          "type": "text",
          "placeholder": "20",
          "value": "20",
          "inputMode": "numeric"
        },
        {
          "id": "nextToken",
          "label": "Next page token (for loading more)",
          "type": "text",
          "placeholder": "Paste token from previous response..."
        }
      ]
    },

